# 向量搜索微服务环境变量配置示例
# 复制此文件并重命名为 .env 进行自定义配置

# 基础配置
PROJECT_NAME="FastAPI 向量搜索微服务"
PROJECT_VERSION="0.1.0"
DEBUG=true

# Milvus 配置
MILVUS_HOST=localhost
MILVUS_PORT=19530
MILVUS_COLLECTION_NAME=questions
MILVUS_INDEX_TYPE=IVF_FLAT
MILVUS_METRIC_TYPE=COSINE
MILVUS_NLIST=1024
MILVUS_BATCH_SIZE=100

# 服务配置
APP_HOST=0.0.0.0
APP_PORT=8000
WORKERS=4

# 缓存配置
CACHE_SIZE=1000
CACHE_TTL=3600  # 秒

# 限流配置
RATE_LIMIT=100/minute

# 监控配置
PROMETHEUS_ENABLED=false